{
  "name": "@kazupon/jts-utils",
  "description": "Opinionated collection of JavaScript / TypeScript utils by @kazupon",
  "version": "0.2.3",
  "publish": {
    "include": ["src/**/*.ts", "package.json", "jsr.json", "README.md", "CHANGELOG.md", "LICENSE"],
    "exclude": ["src/**/*.test.ts", "src/**/*.test-d.ts"]
  },
  "exports": {
    ".": "./src/index.ts",
    "./array": "./src/array/index.ts",
    "./string": "./src/string/index.ts",
    "./observer": "./src/observer/index.ts",
    "./module": "./src/module/index.ts"
  }
}
